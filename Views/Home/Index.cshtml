@{
    ViewData["Title"] = "Home Page";
}

<header class="bg-white py-5">
    <div class="container px-4 px-lg-5 my-5">
        <div class="text-center text-white">
            <h2 class="display-6 fw-bolder text-muted">Shop cua Minh va Truong</h2>
            <p class="lead fw-normal text-muted-50 mb-0">Voi nha tai tro FPT Book Store</p>

        </div>
    </div>
    <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="https://intphcm.com/data/upload/poster-quang-cao-nho-gon.jpg" height="350" width="1500" class="d-block w-100">
            </div>
            <div class="carousel-item">
                <img src="https://vnwriter.net/wp-content/uploads/2018/03/sach-1001-y-tuong.jpg" height="350" width="1500" class="d-block w-100">
            </div>
            <div class="carousel-item">
                <img src="https://thuvien.hocviennhiepanh.com/wp-content/uploads/sach-hoc-chup-anh-san-pham.jpg" height="350" width="1500" class="d-block w-100">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</header>